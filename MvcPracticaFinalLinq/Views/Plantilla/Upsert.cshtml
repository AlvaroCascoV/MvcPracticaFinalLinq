@model Plantilla
<h1>Upsert Plantilla</h1>
@if (Model==null)
{
    <form method="post">
        <label>Hospital</label>
        <input type="text" name="hospitalcod" class="form-control"/>
        <label>Sala</label>
        <input type="text" name="salacod" class="form-control" />
        <label>Empleado</label>
        <input type="text" name="empleadono" class="form-control" />
        <label>Apellido</label>
        <input type="text" name="apellido" class="form-control" />
        <label>Funcion</label>
        <input type="text" name="funcion" class="form-control" />
        <label>Turno</label>
        <input type="text" name="turno" class="form-control" />
        <label>Salario</label>
        <input type="text" name="salario" class="form-control" />
        <button class="btn btn-success">Upsert</button>
    </form>
    <h2>@ViewData["MENSAJEUP"]</h2>
}
else
{

    <form method="post">
        <label>Hospital</label>
        <input type="text" name="hospitalcod" value="@Model.hospitalcod" class="form-control" />
        <label>Sala</label>
        <input type="text" name="salacod" value="@Model.salacod" class="form-control" />
        <label>Empleado</label>
        <input type="text" name="empleadono" value="@Model.empleadono" class="form-control" />
        <label>Apellido</label>
        <input type="text" name="apellido" value="@Model.apellido" class="form-control" />
        <label>Funcion</label>
        <input type="text" name="funcion" value="@Model.funcion" class="form-control" />
        <label>Turno</label>
        <input type="text" name="turno" value="@Model.turno" class="form-control" />
        <label>Salario</label>
        <input type="text" name="salario" value="@Model.salario" class="form-control" />
        <button class="btn btn-success">Upsert</button>
    </form>
    <h2>@ViewData["MENSAJEUP"]</h2>
}